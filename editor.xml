<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_VIRTUAL_SPACE" value="true" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern useCustomColors="true" pattern="\btodo\b\s\[ww\].*">
      <option name="FOREGROUND" value="31a1bc" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="31a1bc" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\btodo\b\s* ww.*">
      <option name="FOREGROUND" value="31a1bc" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="(\b.*todo\b\s\[ww\].*GIT\s*)">
      <option name="FOREGROUND" value="8b0000" />
      <option name="BACKGROUND" value="ffffff" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <filter name="@ ww">
      <pattern index="1" />
      <pattern index="2" />
    </filter>
    <filter name="GIT">
      <pattern index="3" />
    </filter>
  </component>
</application>