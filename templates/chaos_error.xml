<templateSet group="chaos.error">
  <template name="thUnsup" value="throw new java.lang.UnsupportedOperationException(&quot;$CLASS_NAME$.$METHOD_NAME$: $MESSAGE$&quot;);" description="Throw UnsupportedOperationException" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="false" />
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="MESSAGE" expression="enum(&quot;@toDo&quot;,  &quot;MIG#09&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>