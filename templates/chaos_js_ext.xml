<templateSet group="chaos.js.ext">
  <template name="extClass" value="/**&#10; * @class $PACKAGE$.$NAME$&#10; * @extends $EXTENDS_FQN$&#10; */&#10;Ext.define('$PACKAGE$.$NAME$', {&#10;    extend: '$EXTENDS_FQN$',&#10;    config : {} &#10;    $END$      &#10;});" description="Ext.define" toReformat="false" toShortenFQNames="true">
    <variable name="PACKAGE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="NAME" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <variable name="EXTENDS_FQN" expression="jsQualifiedClassName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="extCallB" value="{&#10;    failure: function (records, operation) {    &#10;    },&#10;    success: function (records, operation) {&#10;        $END$&#10;    }&#10;}" description="Ext Callback" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_EXPRESSION" value="true" />
    </context>
  </template>
</templateSet>