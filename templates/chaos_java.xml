<templateSet group="chaos.java">
  <template name="toDo" value="// @toDo [$user$] $END$" description="@toDo[...]" toReformat="false" toShortenFQNames="true">
    <variable name="user" expression="enum(&quot;ww&quot;, user())" defaultValue="ww" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="GROOVY_STATEMENT" value="true" />
      <option name="GROOVY_DECLARATION" value="true" />
    </context>
  </template>
  <template name="//---" value="    // -----------------------------------------------------------------------------------------------------------------&#10;    // $SECTION$&#10;    $END$&#10;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="SECTION" expression="enum(&quot;Access&quot;,&quot;Construction&quot;,&quot;Utils&quot;,&quot;Implementation&quot;,&quot;Events&quot;,&quot;Listener&quot;, &quot;Debug&quot;, &quot;Test&quot;, &quot;Service&quot;, &quot;Abstractions&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="false" />
      <option name="XML" value="false" />
      <option name="CSS" value="false" />
      <option name="GROOVY" value="false" />
      <option name="GROOVY_STATEMENT" value="true" />
      <option name="MAVEN" value="false" />
      <option name="ASPECTJ" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="JSP" value="false" />
      <option name="HAML" value="false" />
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="//==" value="    //==================================================================================================================&#10;    //                                         $SECTION$&#10;    //------------------------------------------------------------------------------------------------------------------&#10;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="SECTION" expression="" defaultValue="&quot;Getter / Setter&quot;" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="false" />
      <option name="XML" value="false" />
      <option name="CSS" value="false" />
      <option name="GROOVY" value="false" />
      <option name="GROOVY_STATEMENT" value="true" />
      <option name="GROOVY_DECLARATION" value="true" />
      <option name="MAVEN" value="false" />
      <option name="ASPECTJ" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="JSP" value="false" />
      <option name="HAML" value="false" />
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>